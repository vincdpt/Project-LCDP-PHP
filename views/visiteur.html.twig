{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
<table class="table table-striped">
  <thead>
    <th>Id du visiteur</th>
    <th>Id du secteur</th>
    <th>Nom</th>
    <th>Pr√©nom</th>
    <th>Login</th>
    <th>Mot de passe</th>
    <th>Adresse</th>
    <th>CP</th>
    <th>Ville</th>
    <th>Date embauche</th>
  </thead>
  <tbody>
{% for visiteur in visiteurs %}
<tr>
    <th style="color: #337ab7;">{{ visiteur.idVisiteur() }}</th>
